(** Function declaration inlining annotations on parameters. *)

type t =
  | Inline_when_known
  | Default_inline

val print : Format.formatter -> t -> unit

val equal : t -> t -> bool

val is_default : t -> bool

val from_lambda : Lambda.parameter_attribute -> t
